{
  "name": "diamonds",
  "version": "0.0.1",
  "description": "Tools for deploying and interfacing with ERC-2535 Diamond Proxies",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "authors": [
    "SuperGenius",
    "Am0rfu5"
  ],
  "license": "MIT",
  "keywords": [
    "ethereum",
    "smart-contracts",
    "hardhat",
    "hardhat-plugin",
    "diamond",
    "diamond-proxy",
    "diamond-upgradeable",
    "blockchain"
  ],
  "repository": "github:GeniusVentures/diamonds",
  "bugs": {
    "url": "https://github.com/GeniusVentures/diamonds/issues"
  },
  "scripts": {
    "test": "hardhat test",
    "test:unit": "hardhat test test/unit/**/*.test.ts",
    "test:integration": "hardhat test test/integration/**/*.test.ts",
    "test:coverage": "hardhat coverage",
    "test:performance": "hardhat test test/integration/performance/**/*.test.ts",
    "compile": "hardhat compile",
    "clean": "hardhat clean && rimraf .tmp-test*",
    "lint:fix": "prettier --write 'src/**/*.{js,ts}' 'test/**/*.{js,ts}' && tslint --fix --config tslint.json --project tsconfig.json",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "yarn build",
    "defender:deploy": "ts-node scripts/defender-cli.ts deploy",
    "defender:upgrade": "ts-node scripts/defender-cli.ts upgrade",
    "defender:status": "ts-node scripts/defender-cli.ts status",
    "defender:verify": "ts-node scripts/defender-cli.ts verify",
    "defender:init": "ts-node scripts/defender-cli.ts init"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@ethersproject/abi": "^5.8.0",
    "@ethersproject/bytes": "^5.8.0",
    "@ethersproject/providers": "^5.8.0",
    "@gnus.ai/contracts-upgradeable-diamond": "=4.5.0",
    "@gnus.ai/hardhat-diamonds": "git+https://github.com/GeniusVentures/hardhat-diamonds.git",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/defender-sdk": "^2.6.0",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.2.0",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^9.1.0",
    "@types/node": "^18",
    "@types/sinon-chai": "^3.2.3",
    "@types/uint32": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "abi2oas": "^0.1.4",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "commander": "^9.4.1",
    "contracts-starter": "git+https://github.com/mudgen/diamond-2-hardhat.git",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.4",
    "hardhat": "^2.24.0",
    "hardhat-abi-exporter": "^2.8.0",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-multichain": "git+https://github.com/GeniusVentures/hardhat-multchain.git",
    "jest-mock": "^29.7.0",
    "mocha": "^9.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "rimraf": "^6.0.1",
    "sinon": "^20.0.0",
    "solhint": "^3.3.7",
    "ts-node": "^10.4.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^4.5.5",
    "uint32": "^0.2.1",
    "web3": "^4.16.0",
    "web3-core": "^4.7.1",
    "web3-utils": "^4.3.3",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "ethers": "^5.0.0",
    "hardhat": "^2.0.0",
    "ts-node": "^8.0.0",
    "typescript": "^4.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
